{
  "name": "postman2openapi-web",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "rimraf dist ../pkg && NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.js --mode production",
